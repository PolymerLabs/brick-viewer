{
  "name": "brick-viewer",
  "version": "1.0.0",
  "description": "A simple web component",
  "main": "brick-viewer.js",
  "module": "brick-viewer.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "lit-analyzer && eslint 'src/**/*.ts'",
    "format": "prettier src/* --write",
    "docs": "npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/brick-viewer.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "web-dev-server --root-dir=docs --node-resolve --watch",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "web-dev-server --node-resolve --watch",
    "checksize": "rollup -c ; cat brick-viewer.bundled.js | gzip -9 | wc -c ; rm brick-viewer.bundled.js"
  },
  "keywords": [
    "web component",
    "lit-element",
    "typescript"
  ],
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@material/mwc-icon-button": "^0.18.0",
    "@material/mwc-linear-progress": "^0.18.0",
    "@material/mwc-slider": "^0.18.0",
    "lit-element": "^2.4.0",
    "three": "^0.114.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@material/mwc-list": "^0.18.0",
    "@material/mwc-ripple": "^0.18.0",
    "@material/mwc-select": "^0.18.0",
    "@open-wc/testing": "^3.1.7",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@web/dev-server": "^0.1.35",
    "chai": "^4.3.7",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "lit-analyzer": "^1.2.1",
    "mocha": "^10.2.0",
    "prettier": "^1.19.1",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.1",
    "typescript": "^4.9.4",
    "web-component-analyzer": "^1.1.6"
  }
}
